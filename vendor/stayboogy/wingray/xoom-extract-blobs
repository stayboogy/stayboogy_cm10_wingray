# JellyBean blobs necessary for the Moto Xoom Wingray

mkdir blobs
cd blobs
mkdir app
cd app
adb pull system/app/MotoLocationProxy.apk
adb pull system/app/StingrayProgramMenu.apk
adb pull system/app/StingrayProgramMenuSystem.apk
cd ..
mkdir bin
cd bin
adb pull system/bin/akmd2
adb pull system/bin/batch
adb pull system/bin/brcm_guci_drv
adb pull system/bin/ftmipcd
adb pull system/bin/location
adb pull system/bin/tcmd
cd ..
mkdir etc
cd etc
adb pull system/etc/cpcap_gain.bin
adb pull system/etc/gps.conf
adb pull system/etc/gpsconfig.xml
adb pull system/etc/location.cfg
adb pull system/etc/motorola/12m/key_code_map.txt
adb pull system/etc/motorola/12m/tcmd_leds.cfg
adb pull system/etc/security/suplcerts.bks
adb pull system/etc/voip_aud_params.bin
cd ..
mkdir lib
cd lib
mkdir hw
cd hw
adb pull system/lib/hw/camera.stingray.so
adb pull system/lib/hw/gps.stingray.so
adb pull system/lib/hw/gralloc.tegra.so
adb pull system/lib/hw/hwcomposer.tegra.so
cd ..
adb pull system/lib/libmoto_ecnswrapper.so
adb pull system/lib/libtpa.so
adb pull system/lib/libtpa_core.so
adb pull system/lib/libpkip.so
cd ..
cd etc
mkdir firmware
cd firmware
adb pull system/etc/firmware/nvmm_aacdec.axf
adb pull system/etc/firmware/nvddk_audiofx_core.axf
adb pull system/etc/firmware/nvddk_audiofx_transport.axf
adb pull system/etc/firmware/nvmm_adtsdec.axf
