adb pull system/etc/firmware/nvmm_h264dec.axf
adb pull system/etc/firmware/nvmm_jpegdec.axf
adb pull system/etc/firmware/nvmm_jpegenc.axf
adb pull system/etc/firmware/nvmm_manager.axf
adb pull system/etc/firmware/nvmm_mp2dec.axf
adb pull system/etc/firmware/nvmm_mp3dec.axf
adb pull system/etc/firmware/nvmm_mpeg4dec.axf
adb pull system/etc/firmware/nvmm_reference.axf
adb pull system/etc/firmware/nvmm_service.axf
adb pull system/etc/firmware/nvmm_sorensondec.axf
adb pull system/etc/firmware/nvmm_sw_mp3dec.axf
adb pull system/etc/firmware/nvmm_wavdec.axf
adb pull system/etc/firmware/nvrm_avp.bin
cd ..
cd ..
cd lib
mkdir egl
cd egl
adb pull system/lib/egl/libEGL_perfhud.so
adb pull system/lib/egl/libEGL_tegra.so
adb pull system/lib/egl/libGLESv1_CM_perfhud.so
adb pull system/lib/egl/libGLESv1_CM_tegra.so
adb pull system/lib/egl/libGLESv2_perfhud.so
adb pull system/lib/egl/libGLESv2_tegra.so
cd ..
adb pull system/lib/libcgdrv.so
adb pull system/lib/libnvddk_2d.so
adb pull system/lib/libnvddk_2d_v2.so
adb pull system/lib/libnvddk_audiofx.so
adb pull system/lib/libnvdispatch_helper.so
adb pull system/lib/libnvdispmgr_d.so
